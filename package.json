  "scripts": {
    "build:client": "webpack --config webpack.jenkins.config.js",
    "build:demo": "webpack --config webpack.jenkins.demo.config.js",
    "build:next": "next build ./server/src/next",
    "next:start": "next ./server/src/next",
    "build:server": "tsc --project ./server/tsconfig.json",
    "dev": "yarn install && concurrently --kill-others \"webpack-dev-server --config webpack.local.config.js\" \"TS_NODE_PROJECT=./server/tsconfig.json nodemon --legacy-watch --inspect=0.0.0.0:9229 --watch server --watch videx --exec ts-node -r tsconfig-paths/register ./server/src/index.ts\"",
    "precommit": "lint-staged",
    "prettier:all": "prettier --write \"./**/{*.ts,*.tsx,*.js,*.jsx,*.json,*.scss,*.css,*.md}\"",
    "server": "TS_NODE_PROJECT=./server node -r tsconfig-paths/register ./server/src/index.js",
    "test:client": "jest --silent --config=./jest.client.config.json",
    "test:prettier": "prettier --list-different \"./**/{*.ts,*.tsx,*.js,*.jsx,*.json,*.scss,*.css,*.md}\"",
    "test:server": "jest --forceExit --silent --runInBand --no-cache --config=./jest.server.config.json",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  }
